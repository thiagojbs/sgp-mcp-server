# SGP API Configuration
SGP_BASE_URL=https://demo.sgp.net.br/api
SGP_USERNAME=your_username
SGP_PASSWORD=your_password
SGP_API_TOKEN=your_api_token
SGP_APP_NAME=sgp-mcp-server

# Server Configuration
PORT=3000
NODE_ENV=production

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_MAX_KEYS=1000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS_BASIC=100
RATE_LIMIT_MAX_REQUESTS_TOKEN=300
RATE_LIMIT_MAX_REQUESTS_CPF_CNPJ=50

# Logging
LOG_LEVEL=info